text/html;              w3m -v -F -T text/html %s; nametemplate=%s.html; needsterminal
text/html;              w3m -I %{charset} -T text/html; copiousoutput;

image/gif;              mpv --loop-file --no-terminal %s &;
image/*;                nohup feh -F %s &;
application/pdf;        nohup zathura %s &;
