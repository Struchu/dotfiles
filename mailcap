text/html;              lynx %s -lss=~/.config/lynx/lynx.lss; nametemplate=%s.html; needsterminal
text/html;              lynx -dump %s ; nametemplate=%s.html; copiousoutput

image/gif;              mpv --loop-file --no-terminal %s &;
image/*;                nohup feh -F %s &;
application/pdf;        nohup zathura %s &;
