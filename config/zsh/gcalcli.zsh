# Alias definitions for gcalcli

# Show agenda
alias cala='gcalcli agenda --detail end'
# Show week
alias calw='gcalcli calw'
# Show month
alias calm='gcalcli calm'

# Add new event
alias caln='gcalcli add'
# Delete event from calendar
alias cald='gcalcli delete'
# Edit an event
alias cale='gcalcli edit'
# Search for an event
alias cals='gcalcli search'
